    "source": "http://github.com/makinecim/php-sdk"
  },
  "license"          : "ISC",
  "authors"          : [
    {
      "name"    : "Makinecim",
      "homepage": "http://github.com/makinecim/makinecim-php-sdk/contributors"
    }
  ],
  "autoload"         : {
    "psr-4": {
      "Makinecim\\": "src/Makinecim/"
    }
  },
  "minimum-stability": "dev",
  "require-dev"      : {
    "larapack/dd"      : "dev-master",
    "guzzlehttp/guzzle": "~6.0",
    "vlucas/phpdotenv" : "^2.4.0"
